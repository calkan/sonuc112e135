}

@Article{Shendure2008,
  Title                    = {Next-generation {DNA} sequencing.},
  Author                   = {Jay Shendure and Hanlee Ji},
  Journal                  = {Nat Biotechnol},
  Year                     = {2008},

  Month                    = {Oct},
  Number                   = {10},
