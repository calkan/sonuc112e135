  Timestamp                = {2016.07.26},
  Url                      = {http://dx.doi.org/10.1101/gr.1390403}
}

@Article{Huddleston2016,
  Title                    = {An Incomplete Understanding of Human Genetic Variation.},
  Author                   = {Huddleston, John and Eichler, Evan E.},
