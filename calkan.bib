@Article{Kitzman2012,
  Title                    = {Noninvasive whole-genome sequencing of a human fetus.},
  Author                   = {Kitzman, Jacob O. and Snyder, Matthew W. and Ventura, Mario and Lewis, Alexandra P. and Qiu, Ruolan and Simmons, Lavone E. and Gammill, Hilary S. and Rubens, Craig E. and Santillan, Donna A. and Murray, Jeffrey C. and Tabor, Holly K. and Bamshad, Michael J. and Eichler, Evan E. and Shendure, Jay},
  Journal                  = {Sci Transl Med},
  Year                     = {2012},

  Month                    = {Jun},
  Number                   = {137},
  Pages                    = {137ra76},
  Volume                   = {4},

  Abstract                 = {Analysis of cell-free fetal DNA in maternal plasma holds promise for the development of noninvasive prenatal genetic diagnostics. Previous studies have been restricted to detection of fetal trisomies, to specific paternally inherited mutations, or to genotyping common polymorphisms using material obtained invasively, for example, through chorionic villus sampling. Here, we combine genome sequencing of two parents, genome-wide maternal haplotyping, and deep sequencing of maternal plasma DNA to noninvasively determine the genome sequence of a human fetus at 18.5 weeks of gestation. Inheritance was predicted at 2.8 × 10(6) parental heterozygous sites with 98.1\% accuracy. Furthermore, 39 of 44 de novo point mutations in the fetal genome were detected, albeit with limited specificity. Subsampling these data and analyzing a second family trio by the same approach indicate that parental haplotype blocks of ~300 kilo-base pairs combined with shallow sequencing of maternal plasma DNA is sufficient to substantially determine the inherited complement of a fetal genome. However, ultradeep sequencing of maternal plasma DNA is necessary for the practical detection of fetal de novo mutations genome-wide. Although technical and analytical challenges remain, we anticipate that noninvasive analysis of inherited variation and de novo mutations in fetal genomes will facilitate prenatal diagnosis of both recessive and dominant Mendelian disorders.},
  Doi                      = {10.1126/scitranslmed.3004323},
  Institution              = {Department of Genome Sciences, University of Washington, Seattle, WA 98195, USA.},
  Keywords                 = {DNA, blood/genetics; Female; Fetus, metabolism; Gestational Age; Haplotypes, genetics; High-Throughput Nucleotide Sequencing; Humans; Pregnancy; Prenatal Diagnosis, methods},
