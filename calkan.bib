  Timestamp                = {2013.11.21},
  Url                      = {http://www.biomedcentral.com/1471-2164/14/S1/S13}
}

@Article{Xin2016,
  Title                    = {Optimal seed solver: optimizing seed selection in read mapping.},
  Author                   = {Xin, Hongyi and Nahar, Sunny and Zhu, Richard and Emmons, John and Pekhimenko, Gennady and Kingsford, Carl and Alkan, Can and Mutlu, Onur},
  Journal                  = {Bioinformatics},
