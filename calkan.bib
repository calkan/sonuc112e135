  Institution              = {Section on Gene Structure and Disease, Laboratory of Molecular and Cellular Biology, National Institute of Diabetes, Digestive and Kidney Diseases, National Institutes of Health, Bethesda, Maryland 20892-0830, USA. ku@helix.nih.gov},
