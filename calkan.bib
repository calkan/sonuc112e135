@Article{Warren2007,
  Title                    = {Assembling millions of short {DNA} sequences using {SSAKE}},
  Author                   = {René L Warren and Granger G Sutton and Steven J M Jones and Robert A Holt},
  Journal                  = {Bioinformatics},
