@Article{McCarroll2006,
  Title                    = {Common deletion polymorphisms in the human genome.},
  Author                   = {McCarroll, Steven A. and Hadnott, Tracy N. and Perry, George H. and Sabeti, Pardis C. and Zody, Michael C. and Barrett, Jeffrey C. and Dallaire, Stephanie and Gabriel, Stacey B. and Lee, Charles and Daly, Mark J. and Altshuler, David M. and , International HapMap Consortium},
  Journal                  = {Nat Genet},
