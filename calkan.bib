  Institution              = {National Biodefense Analysis and Countermeasures Center, Frederick, Maryland, USA. korens@nbacc.net},
  Keywords                 = {Algorithms; Bacteria, genetics; Bacteriophages, genetics; Computational Biology, methods; RNA, genetics; Sequence Analysis, RNA, methods; Transcriptome, genetics; Zea mays, genetics},
  Language                 = {eng},
  Medline-pst              = {epublish},
