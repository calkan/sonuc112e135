@Article{Kitzman2011,
  Title                    = {Haplotype-resolved genome sequencing of a {Gujarati Indian} individual.},
  Author                   = {Jacob O Kitzman and Alexandra P Mackenzie and Andrew Adey and Joseph B Hiatt and Rupali P Patwardhan and Peter H Sudmant and Sarah B Ng and Can Alkan and Ruolan Qiu and Evan E Eichler and Jay Shendure},
