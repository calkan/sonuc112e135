  Owner                    = {calkan},
  Pii                      = {gr.115956.110},
  Pmid                     = {21131385},
  Timestamp                = {2011.06.17},
  Url                      = {http://dx.doi.org/10.1101/gr.115956.110}
}

@Article{Hormozdiari2007,
