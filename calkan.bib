@Article{Antonacci2014,
  Title                    = {Palindromic {GOLGA8} core duplicons promote chromosome 15q13.3 microdeletion and evolutionary instability.},
  Author                   = {Antonacci, Francesca and Dennis, Megan Y. and Huddleston, John and Sudmant, Peter H. and Steinberg, Karyn Meltz and Rosenfeld, Jill A. and Miroballo, Mattia and Graves, Tina A. and Vives, Laura and Malig, Maika and Denman, Laura and Raja, Archana and Stuart, Andrew and Tang, Joyce and Munson, Brenton and Shaffer, Lisa G. and Amemiya, Chris T. and Wilson, Richard K. and Eichler, Evan E.},
