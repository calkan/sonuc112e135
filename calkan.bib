@Article{Jeck2007,
  Title                    = {Extending assembly of short {DNA} sequences to handle error},
  Author                   = {William R Jeck and Josephine A Reinhardt and David A Baltrus and Matthew T Hickenbotham and Vincent Magrini and Elaine R Mardis and Jeffery L Dangl and Corbin D Jones},
