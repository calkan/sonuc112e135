@Article{Sboner2011,
  Title                    = {The real cost of sequencing: higher than you think!},
  Author                   = {Sboner, Andrea and Mu, Xinmeng Jasmine and Greenbaum, Dov and Auerbach, Raymond K. and Gerstein, Mark B.},
