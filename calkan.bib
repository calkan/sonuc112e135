@Article{ORoak2011,
  Title                    = {Exome sequencing in sporadic autism spectrum disorders identifies severe de novo mutations.},
  Author                   = {O'Roak, Brian J. and Deriziotis, Pelagia and Lee, Choli and Vives, Laura and Schwartz, Jerrod J. and Girirajan, Santhosh and Karakoc, Emre and Mackenzie, Alexandra P. and Ng, Sarah B. and Baker, Carl and Rieder, Mark J. and Nickerson, Deborah A. and Bernier, Raphael and Fisher, Simon E. and Shendure, Jay and Eichler, Evan E.},
  Journal                  = {Nat Genet},
