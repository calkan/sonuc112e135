@Article{Volik2006,
  Title                    = {Decoding the fine-scale structure of a breast cancer genome and transcriptome.},
  Author                   = {Stanislav Volik and Benjamin J Raphael and Guiqing Huang and Michael R Stratton and Graham Bignel and John Murnane and John H Brebner and Krystyna Bajsarowicz and Pamela L Paris and Quanzhou Tao and David Kowbel and Anna Lapuk and Dmitri A Shagin and Irina A Shagina and Joe W Gray and Jan-Fang Cheng and Pieter J de Jong and Pavel Pevzner and Colin Collins},
  Journal                  = {Genome Res},
