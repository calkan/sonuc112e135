  Author                   = {Peter H Sudmant and Jacob O Kitzman and Francesca Antonacci and Can Alkan and Maika Malig and Anya Tsalenko and Nick Sampas and Laurakay Bruhn and Jay Shendure and 1000 Genomes Project and Evan E Eichler},
  Journal                  = {Science},
