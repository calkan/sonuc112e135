@Article{Stefansson2005,
  Title                    = {A common inversion under selection in {Europeans}.},
  Author                   = {Stefansson, Hreinn and Helgason, Agnar and Thorleifsson, Gudmar and Steinthorsdottir, Valgerdur and Masson, Gisli and Barnard, John and Baker, Adam and Jonasdottir, Aslaug and Ingason, Andres and Gudnadottir, Vala G. and Desnica, Natasa and Hicks, Andrew and Gylfason, Arnaldur and Gudbjartsson, Daniel F. and Jonsdottir, Gudrun M. and Sainz, Jesus and Agnarsson, Kari and Birgisdottir, Birgitta and Ghosh, Shyamali and Olafsdottir, Adalheidur and Cazier, Jean-Baptiste and Kristjansson, Kristleifur and Frigge, Michael L. and Thorgeirsson, Thorgeir E. and Gulcher, Jeffrey R. and Kong, Augustine and Stefansson, Kari},
  Journal                  = {Nat Genet},
