@Article{Hormozdiari2011,
  Title                    = {Sensitive and fast mapping of di-base encoded reads.},
  Author                   = {Farhad Hormozdiari and Faraz Hach and S. Cenk Sahinalp and Evan E Eichler and Can Alkan},
