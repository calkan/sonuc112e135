  Pmid                     = {24307552},
  Timestamp                = {2015.01.16},
  Url                      = {http://dx.doi.org/10.1101/gr.162883.113}
}

@Article{Chen2007a,
  Title                    = {{PolyScan:} an automatic indel and {SNP} detection approach to the analysis of human resequencing data.},
  Author                   = {Ken Chen and Michael D McLellan and Li Ding and Michael C Wendl and Yumi Kasai and Richard K Wilson and Elaine R Mardis},
