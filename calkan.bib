  Institution              = {Institute of Plant Molecular Biology, Biology Centre ASCR, Branišovská 31, České Budějovice, CZ-37005, Czech Republic.},
  Keywords                 = {Algorithms; Cluster Analysis; DNA, chemistry; Eukaryota, genetics; Genome; High-Throughput Nucleotide Sequencing; Internet; Phylogeny; Repetitive Sequences, Nucleic Acid; Sequence Analysis, DNA; Software},
