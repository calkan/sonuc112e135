@Article{Alkan2014,
  Title                    = {Whole genome sequencing of Turkish genomes reveals functional private alleles and impact of genetic interactions with {Europe}, {Asia} and {Africa}.},
  Author                   = {Alkan, Can and Kavak, Pinar and Somel, Mehmet and Gokcumen, Omer and Ugurlu, Serkan and Saygi, Ceren and Dal, Elif and Bugra, Kuyas and G{\"{u}}ng{\"{o}}r, Tunga and Sahinalp, S Cenk and Oz{\"{o}}ren, Nesrin and Bekpen, Cemalettin},
